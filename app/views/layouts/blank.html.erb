  <%= csrf_meta_tags %>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  

  <!-- CSS -->
 

  <!--[if lte IE 9]>
    <link href="assets/css/main-ie.css" rel="stylesheet" type="text/css" />
