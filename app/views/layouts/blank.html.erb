  <![endif]-->

  <!-- Fav and touch icons -->

</head>
<div class= "full-page-wrapper wrapper">
  <div class= "bottom">
    <%= yield %>
  </div>
</div>
  <div class= "footer">
    <%= render "layouts/footer" %>
  </div>
